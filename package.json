{
  "name": "dash_rete",
  "version": "0.0.1",
  "description": "Dash component wrapping rete.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/thisistheplace/dash-rete.git"
  },
  "bugs": {
    "url": "https://github.com/thisistheplace/dash-rete/issues"
  },
  "homepage": "https://github.com/thisistheplace/dash-rete",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components dash_rete -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\.",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Ben Cannell <bean.can.design@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "efficy-rete-context-menu-plugin": "^1.0.0-alpha9",
    "ramda": "^0.26.1",
    "rete": "^1.4.9",
    "rete-area-plugin": "^0.2.1",
    "rete-connection-plugin": "^0.9.0",
    "rete-context-menu-plugin": "^1.0.0-alpha9",
    "rete-react-render-plugin": "^0.3.0",
    "rete-vue-render-plugin": "^1.0.0-rc2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@plotly/dash-component-plugins": "^1.2.0",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-docgen": "^4.1.1",
    "react-dom": "^18.0.0",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "terser-webpack-plugin": "^2.3.0",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.6",
    "webpack-serve": "3.1.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
